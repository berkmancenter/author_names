<div class="col-lg-12">
  <h1>Export Data</h1>
</div>

<div class="col-lg-6">
  <h3>CSV</h3>
  <% unless @csv_export.nil? %>
    <% @questionnaires.each do |q| %>
      <%= link_to "Download #{q.name} CSV", "/uploads/export_#{q.name.gsub(/ /, '-')}.csv" %><br /><br />
	<% end %>
  <% end %>
</div>



			  
